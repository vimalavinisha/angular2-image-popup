{
  "name": "angular-modal-gallery-universal-demo",
  "version": "7.0.0",
  "license": "MIT",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "build:dev": "ng build --aot",
    "build:prod": "ng build --prod --aot --build-optimizer=true",
    "lint": "ng lint",
    "build:client-and-server-bundles": "npm run build:prod && ng run ng-universal-demo:server",
    "build:prerender": "npm run build:client-and-server-bundles && npm run compile:server && npm run generate:prerender",
    "build:ssr": "npm run build:client-and-server-bundles && npm run compile:server",
    "compile:server": "tsc -p src/tsconfig.server.json",
    "generate:prerender": "cd dist && node prerender",
    "serve:prerender": "cd dist/browser && http-server",
    "serve:ssr": "node dist/server"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^7.0.0-rc.0",
    "@angular/common": "^7.0.0-rc.0",
    "@angular/compiler": "^7.0.0-rc.0",
    "@angular/core": "^7.0.0-rc.0",
    "@angular/forms": "^7.0.0-rc.0",
    "@angular/http": "^7.0.0-rc.0",
    "@angular/platform-browser": "^7.0.0-rc.0",
    "@angular/platform-browser-dynamic": "^7.0.0-rc.0",
    "@angular/platform-server": "^7.0.0-rc.0",
    "@angular/router": "^7.0.0-rc.0",
    "@fortawesome/fontawesome-svg-core": "^1.2.2",
    "@fortawesome/free-solid-svg-icons": "^5.2.0",
    "@ks89/angular-modal-gallery": "^7.0.0-alpha.1",
    "@nguniversal/common": "^6.1.0",
    "@nguniversal/express-engine": "^6.1.0",
    "@nguniversal/module-map-ngfactory-loader": "^6.1.0",
    "core-js": "^2.5.7",
    "hammerjs": "^2.0.8",
    "mousetrap": "^1.6.2",
    "reflect-metadata": "^0.1.12",
    "rxjs": "^6.3.3",
    "zone.js": "^0.8.26"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.9.0-rc.0",
    "@angular/cli": "^7.0.0-rc.0",
    "@angular/compiler-cli": "~7.0.0-rc.0",
    "@angular/language-service": "~7.0.0-rc.0",
    "@types/hammerjs": "^2.0.36",
    "@types/mousetrap": "^1.6.0",
    "@types/node": "~10.11.3",
    "codelyzer": "^4.4.4",
    "cpy-cli": "^2.0.0",
    "express": "^4.15.2",
    "http-server": "^0.11.1",
    "regexp-replace-loader": "^1.0.1",
    "ts-loader": "^4.4.2",
    "ts-node": "~7.0.0",
    "tslint": "^5.11.0",
    "typescript": "^3.1.1",
    "webpack-cli": "^3.1.2"
  }
}
