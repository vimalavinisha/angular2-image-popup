{
  "name": "angular-modal-gallery-webpack-demo",
  "version": "3.0.0",
  "author": "Stefano Cappa",
  "description": "based on https://github.com/Ks89/Angular2-webpack2-skeleton",
  "license": "MIT",
  "main": "src/main.ts",
  "homepage": "https://www.stefanocappa.it",
  "private": true,
  "repository": "https://github.com/Ks89/Angular2-webpack2-skeleton",
  "engines": {
    "node": ">=6.0.0",
    "npm": ">=3.8.6"
  },
  "scripts": {
    "link": "npm link angular-modal-gallery",
    "start": "webpack-dev-server --progress --port 8080",
    "prebuild": "npm run clean-dist",
    "build:dev": "webpack --config config/webpack.dev.js --colors --progress --display-error-details --display-cached --profile",
    "build:prod": "webpack --config config/webpack.prod.js --colors --progress --display-error-details --display-cached --profile",
    "build:prod:aot": "npm run clean && webpack --config config/webpack.prod.js --colors --progress --display-error-details --display-cached --profile",
    "build:github:aot": "npm run clean-github-live-demo && webpack --config config/webpack.prod.js --colors --progress --display-error-details --display-cached --profile",
    "clean": "npm run clean-dist && npm run clean-aot && npm run clean-awcache && npm run clean-coverage",
    "clean-dist": "rimraf dist",
    "clean-aot": "rimraf aot",
    "clean-awcache": "rimraf .awcache",
    "clean-coverage": "rimraf coverage",
    "clean-github-live-demo": "rimraf ../../docs",
    "tslint": "tslint",
    "lint": "npm run tslint \"src/**/*.ts\"",
    "pretest": "npm run clean-coverage",
    "test": "karma start --single-run",
    "pree2e": "npm run webdriver:update -- --standalone",
    "e2e:live": "npm run e2e -- --elementExplorer",
    "e2e": "npm run e2e:only",
    "e2e:only": "npm run protractor",
    "protractor": "protractor",
    "webdriver-manager": "webdriver-manager",
    "webdriver:start": "npm run webdriver-manager start",
    "webdriver:update": "npm run webdriver-manager update",
    "coveralls": "coveralls < coverage/lcov.info",
    "codeclimate": "codeclimate-test-reporter < coverage/lcov.info",
    "next-update": "next-update -k true --tldr"
  },
  "dependencies": {
    "@angular/common": "^4.0.0-beta.7",
